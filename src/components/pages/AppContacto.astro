---
import LayoutContacto from '@/layouts/ContactoLayout.astro';

import Formulario from './Formulario.astro';

import Footer from '@/components/Footer.astro';

// Importamos la traducci√≥n de los idiomas
import { getI18n } from '@/i18n/';

const { currentLocale } = Astro;
const i18n = getI18n({ currentLocale });
---

<LayoutContacto
  title={i18n.FORM.SEO_TITLE}
  description={i18n.FORM.SEO_DESCRIPTION}
  icons="/ino.webp"
>
  <Formulario />

  <Footer />
</LayoutContacto>
